<div id="page" ng-app="historyApp">
	<div id="wrapper" backstretch image="images/background.jpg">
		<div class="container">
			<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
				<div class="content well history-content" ng-controller="HistoryController">
					<h1 class="white">Your results the last 30 days</h1>
					<table class="table table-hover">
						<thead>
							<th>Date</th>
							<th>Weight</th>
							<th>BMI</th>
							<th>Category</th>
						</thead>
						<tbody>
							<tr ng-repeat="datapoint in meassurements">
								<td>{{ datapoint.createdAt }}</td>
								<td>{{ datapoint.weight }} kg</td>
								<td>{{ datapoint.bmi }}</td>
								<td>{{ datapoint.category }}</td>
							</tr>
						</tbody>
					</table>
				</div>
			</div>
		</div>
	</div>
</div>