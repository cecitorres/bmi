<div ng-app="historyApp">
	<div class="container" ng-controller="HistoryController">
		<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
			<h1>Last 30 days</h1>
			<table class="table table-hover table-striped">
				<thead>
					<th>Date</th>
					<th>Weight</th>
					<th>BMI</th>
					<th>Category</th>
				</thead>
				<tbody>
					<tr ng-repeat="datapoint in meassurements">
						<td>{{ datapoint.createdAt }}</td>
						<td>{{ datapoint.weight }}</td>
						<td>{{ datapoint.bmi }}</td>
						<td>{{ datapoint.category }}</td>
					</tr>
				</tbody>
			</table>
		</div>
	</div>
</div>